{
  "name": "pinwheel",
  "version": "1.0.0",
  "description": "A Node.js MUD game engine built with Ranvier.",
  "author": "Andrew Zigler <andrewzigler@gmail.com> (https://www.andrewzigler.com)",
  "private": true,
  "repository": "azigler/pinwheel",
  "engines": {
    "node": ">= 8.9.4"
  },
  "license": "MIT",
  "scripts": {
    "start": "gulp && ./pinwheel -v --save=10 --respawn=10",
    "test": "gulp && npm run-script test:coverage",
    "test:coverage": "istanbul --include-all-sources cover _mocha -- -R dot --recursive ./test/**/*.spec.js",
    "build-docs": "jsdoc -c jsdoc.conf",
    "bundle-install": "node setup-bundles"
  },
  "dependencies": {
    "bcryptjs": "^2.4.0",
    "commander": "^2.16.0",
    "express": "^4.16.3",
    "js-yaml": "^3.12.0",
    "longjohn": "^0.2.11",
    "optimist": "^0.6.1",
    "pretty-error": "^2.0.2",
    "rando-js": "^0.1.1",
    "semver": "^5.4.1",
    "sprintf-js": "^1.0.3",
    "sty": "^0.6.1",
    "uuid": "^3.2.1",
    "winston": "^2.3.1",
    "wrap-ansi": "^2.0.0",
    "yaml": "^1.0.0-rc.7"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-todo": "^4.1.0",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-istanbul": "^0.3.0",
    "sinon": "^1.17.5",
    "tui-jsdoc-template": "^1.1.0"
  }
}
